{
  "name": "hugo-templates-docs",
  "version": "1.0.0",
  "type": "documentation",
  "build": {
    "template": "documentation",
    "theme": "compose",
    "components": ["compose-theme"]
  },
  "site": {
    "title": "Hugo Templates Documentation",
    "description": "Template factory framework for Hugo static site generator",
    "baseURL": "https://hugo.info-tech.io",
    "language": "ru",
    "author": "InfoTech.io Team",
    "product_name": "Hugo Templates"
  },
  "content": {
    "source": "./content",
    "destination": "hugo"
  },
  "build_settings": {
    "hugo_version": "0.110.0",
    "enable_analytics": false,
    "enable_search": true
  }
}