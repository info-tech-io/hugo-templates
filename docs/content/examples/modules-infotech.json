{
  "$schema": "../../schemas/modules.schema.json",
  "federation": {
    "name": "InfoTech.io Documentation Federation",
    "baseURL": "https://info-tech-io.github.io",
    "strategy": "download-merge-deploy",
    "build_settings": {
      "parallel": false,
      "cache_enabled": true,
      "performance_tracking": true
    }
  },
  "modules": [
    {
      "name": "corporate-site",
      "source": {
        "repository": "https://github.com/info-tech-io/info-tech",
        "path": "docs",
        "branch": "main"
      },
      "module_json": "module.json",
      "destination": "/",
      "css_path_prefix": "",
      "overrides": {
        "components": []
      }
    },
    {
      "name": "quiz-docs",
      "source": {
        "repository": "https://github.com/info-tech-io/quiz",
        "path": "docs",
        "branch": "main"
      },
      "module_json": "module.json",
      "destination": "/quiz/",
      "css_path_prefix": "/quiz"
    },
    {
      "name": "hugo-templates-docs",
      "source": {
        "repository": "https://github.com/info-tech-io/hugo-templates",
        "path": "docs",
        "branch": "main"
      },
      "module_json": "module.json",
      "destination": "/hugo-templates/",
      "css_path_prefix": "/hugo-templates"
    },
    {
      "name": "web-terminal-docs",
      "source": {
        "repository": "https://github.com/info-tech-io/web-terminal",
        "path": "docs",
        "branch": "main"
      },
      "module_json": "module.json",
      "destination": "/web-terminal/",
      "css_path_prefix": "/web-terminal"
    },
    {
      "name": "info-tech-cli-docs",
      "source": {
        "repository": "https://github.com/info-tech-io/info-tech-cli",
        "path": "docs",
        "branch": "main"
      },
      "module_json": "module.json",
      "destination": "/info-tech-cli/",
      "css_path_prefix": "/info-tech-cli"
    }
  ]
}
